  "scripts": {
    "start": "npm run build && node server.js",
    "prebuild": "(rm -r public || true) && (rm app/config/config-routes.js || true)",
    "build": "brunch build",
    "postinstall": "npm run build",
    "dev": "gulp",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "lint": "eslint .",
    "autofix": "eslint --fix .",
    "codecov": "npm run test && (codecov || true)",
    "validate": "npm ls",
    "initwatson": "cf create-service tone_analyzer standard tone-analyzer-standard && cf create-service tradeoff_analytics standard tradeoff-analytics-standard-service && cf create-service watson_vision_combined free visual-recognition-free && cf create-service speech_to_text standard speech-to-text-service-standard && cf create-service personality_insights tiered personality-insights-service && cf create-service alchemy_api free alchemy-service",
    "deploy": "cf push"
  },

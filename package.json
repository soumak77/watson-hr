    "firebase-reporting": "~0.0.15",
    "glob": "^7.0.3",
    "helmet": "^2.1.0",
    "morgan": "^1.7.0",
    "precommit-hook": "^3.0.0",
    "request": "^2.72.0",
    "vcap_services": "^0.1.7",
    "watson-developer-cloud": "^2.4.3"
  },

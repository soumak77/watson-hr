    "autoprefixer": "^6.4.1",
    "babel-brunch": "~6.0.0",
    "babel-preset-es2015": "~6.3.13",
    "base-apps-router": "^1.2.0",
    "clean-css-brunch": "^2.0.0",
    "css-brunch": "^2.0.0",
    "eslint-brunch": "^3.9.0",
